<% @other_users.each do |user| %>
        <% if !current_user.following?(user) %>
        <div class="bg-light p-1 my-3">
          <div class="">
            <h4 class="text-center d-flex justify-content-between align-items-center">
              <div>
                 <%= profile_image(user, "to-follow-img") %>
                <%= link_to  user.username, user %>
              </div>
              <%= follow_btn(user) %>
            </h4>
          </div>
        </div>
        <% end %>
      <% end %>